///ISO VARIABLES
/**
*WHERE IT GOES: Into the Create Event
*WHAT IT DOES: This script sets the variables needed to handle
*the "Z" axis used for Isometric jumping.
*
*/

z = 0; // Current z position
zspeed = 0; // vertical speed
zheight = 81; //how tall the sprite is
zbottom = 0; //where the floor is
zgravity = 1; //gravity pulling down

friction = .08;

